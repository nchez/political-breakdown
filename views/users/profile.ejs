<h2>Hello <%= user.email %></h2>

<h2>Congress Member Watchlist</h2>

<ul>
  <% userOfficialsArr.forEach(element => { %>
  <li><a href="/officials/<%=element.name%>"> <%= element.name %> </a></li>
  <% }) %>
</ul>

<h2>Stock Watchlist</h2>

<ul>
  <% userStocksArr.forEach(element => { %>
  <li>
    <a href="/stocks/<%=element.symbol%>"> <%= element.name %> </a>
    <form action="/stocks/<%= element.symbol%>/?_method=DELETE" method="post">
      <button type="submit">Delete</button>
    </form>
  </li>
  <% }) %>
</ul>
