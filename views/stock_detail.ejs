<h2><%= name %> Details</h2>
<h3>What Trustworthy Congress Members Traded <%= name%></h3>

<div class="table-wrapper">
  <table>
    <tr>
      <th>Ticker</th>
      <th>Representative Name</th>
      <th>Transaction Type</th>
      <th>Transaction Amount</th>
      <th>Date</th>
    </tr>
    <% sortResponse.forEach(element => { %>
    <tr>
      <td><%= element.Ticker %></td>
      <td><%= element.Representative%></td>
      <td><%= element.Transaction%></td>
      <td><%= element.Range%></td>
      <td><%= element.TransactionDate%></td>
    </tr>
    <% }) %>
  </table>
</div>
